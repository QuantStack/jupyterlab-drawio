    "@jupyterlab/filebrowser": "^0.15.0",
    "@jupyterlab/mainmenu": "^0.4.2",
    "@jupyterlab/launcher": "^0.15.1"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.6.0",
    "fs-extra": "^0.30.0",
    "@phosphor/messaging": "^1.1.0"
  },
  "jupyterlab": {
    "extension": true
  }
